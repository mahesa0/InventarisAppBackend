{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Credentials": "true"
      }
    }
  ],
  "routers": [
    {
      "src": "/users",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "dest": "routes/UserRoute.js"
    }
  ]
}
